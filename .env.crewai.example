# CrewAI Integration Configuration Example
# Copy this file to .env and adjust values as needed

# Master switch for CrewAI functionality
CREWAI_ENABLED=false

# Rollout percentage (0-100) - start small for testing
CREWAI_ROLLOUT_PERCENTAGE=5

# A/B Testing Configuration
CREWAI_AB_TESTING=true
CREWAI_AB_TESTING_SEED=investingdojo

# Quality and Performance Thresholds
CREWAI_QUALITY_THRESHOLD=7.0
CREWAI_ERROR_THRESHOLD=10.0

# Monitoring and Logging
CREWAI_MONITORING=true
CREWAI_METRICS_RETENTION=30

# Fallback Configuration
CREWAI_AUTO_FALLBACK=true
CREWAI_FALLBACK_TIMEOUT=300

# Development/Testing Settings
# Uncomment for development testing:
# CREWAI_ENABLED=true
# CREWAI_ROLLOUT_PERCENTAGE=10
# CREWAI_AB_TESTING=true

# Production Gradual Rollout Settings
# Uncomment for production deployment:
# CREWAI_ENABLED=true
# CREWAI_ROLLOUT_PERCENTAGE=25
# CREWAI_QUALITY_THRESHOLD=8.0